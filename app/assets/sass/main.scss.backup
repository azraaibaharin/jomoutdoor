
.slick-slider {
	margin-bottom: 0;
}

$slides-height: 200px;
.slides {
	position: absolute;
	top: 0;
	bottom: 0;
	right: 0;
	left: 0;
	margin-bottom: 0;
	section {
		// min-height: $slides-height;
		// padding: 40px;
		top: 0;
		height: 100%;
		background-size: cover;
		background-position: 50% 50%;
	}
	h1 {
		color: #fff;
		font-size: 24px;
		font-weight: 300;
		margin: 0;
		padding: 50px 0;
	}
}



